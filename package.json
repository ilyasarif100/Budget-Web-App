{
  "name": "budget-tracker",
  "version": "1.0.0",
  "description": "Personal Budget Tracker with Plaid Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "minify": "terser script.js js/*.js -o dist/app.min.js -c -m --source-map",
    "build": "npm run minify && npm run copy-assets && npm run build-prod-html",
    "build-prod-html": "cp index.prod.html dist/index.html",
    "copy-assets": "mkdir -p dist && mkdir -p dist/css && cp css/styles.css dist/css/ && cp config.js dist/ && cp sw.js dist/ && cp -r js dist/ && mkdir -p dist/workers && cp workers/*.js dist/workers/ 2>/dev/null || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "backup": "node scripts/backup.js",
    "build:analyze": "node scripts/build-analyze.js",
    "build:compress": "node scripts/build-compress.js",
    "build:check": "npm run build && npm run build:analyze",
    "prepare": "husky"
  },
  "keywords": [
    "budget",
    "tracker",
    "plaid",
    "finance"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.19.5",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "plaid": "^24.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "bundlesize": "^0.18.2",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "terser": "^5.26.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
